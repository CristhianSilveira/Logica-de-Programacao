algoritmo "Soma numeros"
var
    N,soma,cont:inteiro
inicio
   cont <- 0
   Escreval("Escreva 7 números para somá-los:")
   Escreval()
   Enquanto (cont < 7) faca
   cont <- cont + 1
   Escreva("Digite o",cont,"° número: ")
   leia(N)
   soma <- soma + N
   fimenquanto
   Escreval()
   Escreva("A soma de todos os números é:",soma)
fimalgoritmo